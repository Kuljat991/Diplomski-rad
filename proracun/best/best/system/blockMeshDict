        points[5]  = point(-15.0, 15.0, -0.5);
        points[6]  = point(-15.0, -15.0, -0.5);
        points[7]  = point(15.0, -15.0, -0.5);
        points[8]  = point(115.0, 50.0, -0.5);
        points[9]  = point(15.0, 50.0, -0.5);
        points[10]  = point(-15.0, 50.0, -0.5);
        points[11]  = point(-45.0, 50.0, -0.5);
        points[12]  = point(-45.0, 15.0, -0.5);
        points[13]  = point(-45.0, -15.0, -0.5);
        points[14]  = point(-45.0, -50.0, -0.5);
        points[15]  = point(-15.0, -50.0, -0.5);
        points[16]  = point(15.0, -50.0, -0.5);
        points[17]  = point(115.0, -50.0, -0.5);
        points[18]  = point(115.0, -15.0, -0.5);
        points[19]  = point(115.0, 15.0, -0.5);
        
        // Duplicate z points
        label sz = points.size();
        points.setSize(2*sz);
        for (label i = 0; i < sz; i++)
        {
            const point& pt = points[i];
            points[i+sz] = point(pt.x(), pt.y(), -pt.z());
        }

        os  << points;
    #};
};


